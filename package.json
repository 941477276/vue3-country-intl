{
  "name": "vue3-country-intl",
  "description": "A vue3-based mobile phone area code selection, nationality selection component（一个基于vue3的手机区号选择、国籍选择组件）",
  "version": "2.0.11",
  "license": "MIT",
  "author": "hearth_learning_money",
  "scripts": {
    "dev": "npm run generate-version && vite",
    "build": "npm run generate-version && vite build",
    "build:esm": "npm run generate-version && rollup -c rollup.build.config.js --format esm",
    "build:cjs": "npm run generate-version && rollup -c rollup.build.config.js --format cjs",
    "build-vue3CountryFlag:esm": "npm run generate-version && rollup -c rollup.vue3CountryFlag.build.config.js --format esm",
    "build-vue3CountryFlag:cjs": "npm run generate-version && rollup -c rollup.vue3CountryFlag.build.config.js --format cjs",
    "generate-version": "node ./generate-version.js",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix --cache",
    "lint": "run-s lint:*"
  },
  "main": "lib/vue3CountryIntl.esm.min.js",
  "browser": "lib/vue3CountryIntl.esm.min.js",
  "module": "lib/vue3CountryIntl.esm.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/941477276/vue3-country-intl.git"
  },
  "homepage": "https://github.com/941477276/vue3-country-intl#readme",
  "dependencies": {
    "easyest-dropdown": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-external-helpers": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@originjs/vite-plugin-require-context": "^1.0.9",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "2.4.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",

    "eslint": "^9.37.0",
    "eslint-plugin-oxlint": "~1.23.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.23.0",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1",

    "ant-design-vue": "^4.2.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "esbuild": "^0.14.5",
    "minimist": "^1.2.5",
    "rollup": "2.50.5",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-esbuild": "4.7.2",
    "rollup-plugin-styles": "3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "6.0.0",
    "vue": "^3.5.22"
  },
  "keywords": [
    "country select",
    "vue3",
    "flag",
    "vue3 flags",
    "vue3 country select",
    "area code select",
    "vue3 area code select",
    "country international",
    "vue3 country international",
    "区号选择",
    "国籍选择",
    "vue3区号选择",
    "vue3国籍选择",
    "国旗"
  ],
  "files": [
    "lib",
    "package.json",
    "README.md",
    "README-CN.md"
